#include <stdio.h>
int D[3];
int d[25][3];
int chk[25];
int N;
int aa(int lvl) {
	if(lvl == N+1) {
		int i, t[3] = {0,0,0};
		for(i = 1; i <= N; i++) {
			if(chk[i] == 0) continue;
			t[0] += d[i][0];
			t[1] += d[i][1];
			t[2] += d[i][2];
		}
		if(t[0] == D[0] && t[1] == D[1] && t[2] == D[2]) return 1;
		else return 0;
	}
	chk[lvl] = 1;
	if(aa(lvl+1) == 1) return 1;
	chk[lvl] = 0;
	if(aa(lvl+1) == 1) return 1;
	return 0;
}
int main() {
	freopen("input2.txt", "r", stdin);
	freopen("output2.txt", "w", stdout);
	int T, i;
	scanf("%d", &T);
	for(int t = 1; t <= T; t++) {
		scanf("%d %d %d", &D[0], &D[1], &D[2]);
		scanf("%d", &N);
		for(i = 1; i <= N; i++) scanf("%d %d %d", &d[i][0], &d[i][1], &d[i][2]);
		printf("Case #%d: ", t);
		if(aa(1) == 1) printf("yes\n");
		else printf("no\n");
	}
	return 0;
}