#include <iostream>
#include <fstream>
using namespace std;
#define maxN 22
int A[maxN],B[maxN],C[maxN];

bool isPossible(int a, int b, int c, int i, int maxI) {
    if(a == 0 && b == 0 && c == 0){
        return true;
    }
    if(a < 0 || b < 0 || c < 0 || i >= maxI)
        return false;
    if(isPossible(a - A[i], b - B[i], c - C[i], i+1, maxI))
        return true;
    else if(isPossible(a,b,c,i+1,maxI))
        return true;
    else
        return false;
}

