    ofstream output;
    ifstream input;
    input.open ("input.txt");
    output.open ("output.txt");
	int T,a,b,c,N;
	input>>T;
	for(int caseNo = 1; caseNo <= T; caseNo++) {
        input>>a>>b>>c;
        input>>N;
        for(int i = 0; i < N; i++) {
            input>>A[i]>>B[i]>>C[i];
        }
        output<<"Case #"<<caseNo<<": "<<((isPossible(a,b,c,0,N)==true)?"yes":"no")<<endl;
	}


	input.close();
	output.close();
	return 0;
}
