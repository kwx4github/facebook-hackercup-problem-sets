#include <iostream>
#include <stdio.h>
#include <string>
#include <string.h>

using namespace std;

int main(int argc, const char * argv[]) {
    int T,cas = 1,N;
    int Gp,Gc,Gf;
    int p[25],c[25],f[25];
    freopen("/Users/l/Documents/programs/hackercup/new_years_resolution.txt","r",stdin);
    freopen("/Users/l/Documents/programs/hackercup/new_years_resolution_output.txt","w",stdout);
    cin>>T;
    while(T--)
    {
        bool flag = false;
        cin>>Gp>>Gc>>Gf;
        cin>>N;
        for(int i=0;i<N;i++)
            cin>>p[i]>>c[i]>>f[i];
        for(int bit = 0;bit<(1<<N);bit++)
        {
            int psum=0,csum=0,fsum=0;
            for(int i=0;i<N;i++)
                if((1<<i) & bit)
                {
                    psum += p[i];
                    csum += c[i];
                    fsum += f[i];
                }
            if(psum == Gp && csum == Gc && fsum == Gf)
            {
                flag = true;
                break;
            }
        }
        cout<<"Case #"<<cas++<<": ";
        if(flag)
            cout<<"yes"<<endl;
        else
            cout<<"no"<<endl;
    }
    return 0;
}
