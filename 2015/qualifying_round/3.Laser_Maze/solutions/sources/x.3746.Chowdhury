char g[105][105];
int mark[105][105];
int p[105][105];
int n,m;
int ax[]={1,0,-1,0};
int ay[]={0,1,0,-1};

bool ok(int i,int j,int st)
{
    bool got=true;

    for(int k=j+1;k<n;k++)
    {
        if(g[i][k]=='#') break;
        else if((g[i][k]>='0' && g[i][k]<='3' ) && ( (g[i][k]-'0' +st)%4==2 ) )
        {
            got=false;
            break;
        }
    }
    for(int k=j-1;k>=0;k--)
    {
        if(g[i][k]=='#') break;
        if((g[i][k]>='0' && g[i][k]<='3' ) && ( (g[i][k]-'0' +st)%4==0 ) )
        {
            got=false;
            break;
        }
    }
    for(int k=i-1;k>=0;k--)
    {
        if(g[k][j]=='#') break;
        if((g[k][j]>='0' && g[k][j]<='3' ) && ( (g[k][j]-'0' +st)%4==1 ) )
        {
            got=false;
            break;
        }
    }
    for(int k=i+1;k<m;k++)
    {
        if(g[k][j]=='#') break;
        if((g[k][j]>='0' && g[k][j]<='3' ) && ( (g[k][j]-'0' +st)%4==3 ) )
        {
            got=false;
            break;
        }
    }
  //  printf("%d %d %c %d\n",i,j,g[i][j],st);
    return got;
}

int bfs(int x,int y)
{
    int ans=-1;
    queue<int>qx;
    queue<int>qy;

    qx.push(x);
    qy.push(y);

    mark[x][y]=0;
    p[x][y]=0;

    while(!qx.empty() )
    {
        int nx=qx.front();
        int ny=qy.front();
//        printf("%d %d\n",nx,ny);
        qx.pop();
        qy.pop();
        if(g[nx][ny]=='G')
        {
            ans=mark[nx][ny];
            break;
        }

        for(int i=0;i<4;i++)
        {
            if(ax[i]+nx>=0 && ax[i]+nx<m && ay[i]+ny>=0 && ay[i]+ny<n)
            {
                int zx=ax[i]+nx;
                int zy=ay[i]+ny;//printf("*** %d %d %c %d\n",zx,zy,g[zx][zy],mark[zx][zy]);
                if(g[zx][zy]>='0' && g[zx][zy]<='3') continue;
                if(g[zx][zy]!='#' && p[zx][zy]<4 && ok(zx,zy,mark[nx][ny]+1))
                {//pause
                    mark[zx][zy]=mark[nx][ny]+1;
                    qx.push(zx);
                    qy.push(zy);
                    p[zx][zy]++;
                }
            }
        }

    }

    return ans;
}

int main()
{
    #ifndef ONLINE_JUDGE
