      //  freopen ( "hack.txt", "w", stdout );
    #endif

    int t,tcase=1;
    int a,b;
    scanf("%d",&t);

    while(t--)
    {
        scanf("%d%d",&m,&n);

        reset(mark,0);
        reset(p,0);

        for(int i=0;i<m;i++)
        {
            scanf("%s",g[i]);
            for(int j=0;j<n;j++)
            {
                if(g[i][j]=='S')
                {
                    a=i;
                    b=j;
                }
                else if(g[i][j]=='>')
                    g[i][j]='0';
                else if(g[i][j]=='v')
                    g[i][j]='1';
                else if(g[i][j]=='<')
                    g[i][j]='2';
                else if(g[i][j]=='^')
                    g[i][j]='3';
            }
        }

        printf("Case #%d: ",tcase++);
        int get=bfs(a,b);

        if(get==-1)
            printf("impossible\n");
        else
            printf("%d\n",get);
    }

    return 0;
}


