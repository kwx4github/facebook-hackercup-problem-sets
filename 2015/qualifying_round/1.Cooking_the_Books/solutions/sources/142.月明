#include <iostream>
#include <cstdio>
#include <cstring>
#include <algorithm>
#include <cmath>
#include <vector>
#include <queue>
#include <map>
#include <set>
#include <ctime>
#include <cstdlib>
#define ll long long
using namespace std;
const int N = 1e5 + 10;
const int mod = 1e9 + 7;
const int inf = ~0U >> 1;
int main()
{
	int T, cas = 0;
	cin >> T;
	while (T--)
	{
		int n, a[100], len = 0;
		cin >> n;
		int ans1 = n, ans2 = n;
		while (n)
		{
			a[++len] = n % 10;
			n /= 10;
		}
		for (int i = len; i >= 1; --i)
		{
			for (int j = i - 1; j >= 1; --j)
			{
				if (!a[j] && i == len) continue;
				int cur = 0;
				swap(a[i], a[j]);
				for (int k = len; k >= 1; --k) cur = cur * 10 + a[k]; 
				ans1 = min(ans1, cur);
				ans2 = max(ans2, cur);
				swap(a[i], a[j]);
			}
		}
		printf("Case #%d: %d %d\n", ++cas, ans1, ans2);
	}
    return 0;
}
