//int dx[]={1,0,-1,0};int dy[]={0,1,0,-1}; //4 Direction
//int dx[]={1,1,0,-1,-1,-1,0,1};
//int dy[]={0,1,1,1,0,-1,-1,-1};//8 direction
//int dx[]={2,1,-1,-2,-2,-1,1,2};
//int dy[]={1,2,2,1,-1,-2,-2,-1};//Knight Direction
// scanf("%d",&n);
int main()
{
//    READ("cooking_the_books.txt");
//    WRITE("out.txt");
    int T;
    char s[100];
    scanf("%d",&T);
    FOR(t,1,T)
    {
        scanf("%s",s);
        //pr(s);
        int len=strlen(s);
        string mx=s,mn=s;
        FOR(i,0,len-1)
        {
            FOR(j,i+1,len-1)
            {
                swap(s[i],s[j]);
                if(s<mn&&s[0]!='0')mn=s;
                if(s>mx)mx=s;
                swap(s[i],s[j]);
            }
        }
        printf("Case #%d: %s %s\n",t,mn.c_str(),mx.c_str());
    }
	return 0;
}


