#include<iostream>
#include<string>
#include<algorithm>
#include<vector>
using namespace std;
int main()
{
	int t,len;
	string str;
	cin >> t;
	for(int i = 1; i <= t; i++)
	{
		cin >> str;
		len = str.length();
		cout << "Case #" << i << ":"<< "\t";
		cout << str<<"\t";
		sort(str.begin(),str.end());
		for (int j = len; j >= 0; j--)
			cout << str[j];
			cout<<endl;
	}
	return 0;
}