#define debug(a...)          {cout<<" #--> ";dbg,a; cout<<endl;}

struct debugger
{
    ///Collected from rudradevbasak
    template<typename T> debugger& operator , (const T v)
    {
        cout<<v<<" ";
        return *this;
    }
} dbg;


const double pi=acos(-1.0);
const double eps=1e-7;

template<class TT>TT sqr(TT a){return a*a;}
template<class TT>TT abs(TT a){if(a<0)  return -a;return a;}
typedef pair<int,int> pii;



int con(string &s)
{
    int x = 0;
    for(int i=0;i<SZ(s);i++)
        x= x*10 + s[i]-'0';
    return x;
}


string toStr(long long v)
{
    stringstream ss;
    ss<<v;
    return ss.str();
}




int main()
{
//    freopen("in.txt","r",stdin);
    freopen("cooking_the_books.txt","r",stdin);
    freopen("out.txt","w",stdout);

    int i,j,k,tks=1,ks=1,n,mx,mn;
    string in;

    scanf("%d",&tks);

    while(tks--)
    {
        scanf("%d",&n);

        if(n<10)
        {
            printf("Case #%d: %d %d\n",ks++,n,n);
            continue;
        }
        mx=mn=n;
        in = toStr(n);
        n = SZ(in);
        for(int i=0;i<n;i++)
            for(int j=i+1;j<n;j++)
            {
                string tmp = in;
                swap(tmp[i],tmp[j]);
                int v = con(tmp);
                if(v<pow(10,n-1))   continue;
                mx = max(mx,v);
                mn = min(mn,v);
            }

        printf("Case #%d: %d %d\n",ks++,mn,mx);
    }

    return 0;
}




