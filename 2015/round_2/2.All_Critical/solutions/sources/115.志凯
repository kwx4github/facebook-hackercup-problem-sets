#endif

double dp[50];
int main() {
	int T;
	cin >> T;
	rep(cas, T) {
		double p;
		cin >>p;
		dp[0] = 0;
		for (int i = 1; i <= 20; i++) {
			double tmp = 0;
			for (int msk = 1; msk < (1<<i); msk++) {
				int c = 0;
				double pp = 1;
				for (int j = 0; j < i; j++) {
					if (msk & (1<<j)) {
						pp *= p;
						c++;
					}
					else pp *= (1-p);
				}
				tmp += pp * dp[i - c];
			}
			double k = 1;
			rep(j, i) k *= (1-p);
			dp[i] = (tmp+1) / (1-k);
		}
		printf("Case #%d: %.5lf\n", cas+1, dp[20]);
	}
	return 0;
}
