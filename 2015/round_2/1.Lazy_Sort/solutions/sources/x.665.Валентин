int T;
int n;
int mas[50004];
int pos[50004];

int main()
{

#ifndef ONLINE_JUDGE
#ifdef OUT
	freopen("input.txt", "r", stdin);
	freopen("output.txt", "w", stdout); 
#endif

#ifdef FILES
	freopen(FILES".in", "r", stdin);
	freopen(FILES".out", "w", stdout); 
#endif
#endif

	//ios_base::sync_with_stdio(false);
	srand(time(NULL));
	time_t start = clock();	

	scanf("%d", &T);

	for(int t = 1; t <= T; t++)
	{
		printf("Case #%d: ", t);
		
		scanf("%d", &n);

		for(int i = 0; i < n; i++)
		{
			scanf("%d", &mas[i]);
		}

		bool ch = false;

		for(int last = 0; last <= n && ch == false; last++)
		{
			int l = 0;
			int r = n - 1;

			int prv1 = last + 1;
			int prv2 = last;

			ch = true;

			for(int i = 0; i < n && ch; i++)
			{
				if(mas[l] == prv1 - 1)
				{
					prv1--;
					l++;
					continue;
				}
				if(mas[l] == prv2 + 1)
				{
					prv2++;
					l++;
					continue;
				}
				if(mas[r] == prv1 - 1)
				{
					prv1--;
					r--;
					continue;
				}
				if(mas[l] == prv2 + 1)
				{
					prv2++;
					r--;
					continue;
				}
				ch = false;
			}
		}

		puts(ch ? "yes" : "no");
	}

    
#ifndef ONLINE_JUDGE
	fprintf(stderr, "\n%0.3lf\n", (clock() - start) * 1.0 / CLOCKS_PER_SEC);
#endif

	return 0;
}