#include<string.h>
/*
*	You got a dream, you gotta protect it.
*/ 
using namespace std ;
long long dp [2222][2222];
int x , y ; 
long long calc ( int a , int b ) {
	if ( a == x && b == y )  return  1; 
	if ( a > x || b > y ) return 0 ;
	if ( dp[a][b] != -1 )  return dp[a][b];
	long long r = 0 ; 
	if ( a -b > 1  ) 
		r= (r + calc ( a , b+1 ) ) % 1000000007;
	r=  (r + calc ( a+1 , b ) )% 1000000007;
	return dp[a][b] = r % 1000000007; 
}
long long calc2( int a , int b ) {

	if ( b == y )  return  1; 
	if ( a > x || b > y ) return 0 ;
	long long  r = 0 ;
	if ( dp[a][b] != -1 )  return dp[a][b];
	if ( b > a ) 
		r= (r + calc2( a+1 , b ) ) % 1000000007 ; 
	r= (r + calc2(a , b+1 ))% 1000000007 ; 
	return dp [a][b] = r % 1000000007 ; 
}
int  main()
{
	freopen("input.in" , "rt" , stdin);
	freopen ("myfile.txt","w",stdout);
	int t , tt = 1; 
	char ch ; 
	cin >> t ; 
	while ( t-- ) {
		cin >> x >> ch >> y ; 
		memset(dp , -1  , sizeof ( dp )) ;
		long long int  r1 = calc ( 1 , 0 ) ;
		memset(dp , -1  , sizeof ( dp )) ;
		long long int  r2  = calc2(0,1) ;
		printf("Case #%d: ", tt++ ) ; 
		cout << r1 <<  " " << r2 << "\n";
	}
	return 0 ;
}