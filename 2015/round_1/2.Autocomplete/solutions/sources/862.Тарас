#include<stdio.h>
#include<iostream>
#include<iomanip>
#include<string>
#include<vector>
#include<set>
#include<stack>
#include<list>
#include<queue>
#include<deque>
#include<bitset>
#include<map>
#include<algorithm>
#include<cmath>
#include<numeric>

#pragma comment (linker, "/STACK:640000000")
#define INF 10000000000 //1e10
#define EPS 1e-6
#define PI 3.1415926535897932384626433832795028841971
#define mp make_pair
#define pb push_back
#define pf push_front
#define ppf pop_front
#define ppb pop_back
#define x first
#define y second
#define pii pair<int,int>
#define pdd pair<double,double>
#define FOR(i,a,b) for (int _n(b), i(a); i <= _n; ++i)
#define FORD(i,a,b) for(int i=(a),_b=(b);i >= _b; --i)
#define all(c) (c).begin(), (c).end()
#define SORT(c) sort(all(c))
#define rep(i,n) FOR(i,0,(n)-1)
#define VI vector <int>
#define VVI vector <VI>
#define ll long long
#define sqr(x) ((x)*(x))


#define MAX 1000001
#define MOD 1000000007
using namespace std;
const int Z = 'z'-'a'+1;

int T[MAX][Z];
char s[1000001];

int main()
{
	freopen("in.txt", "r", stdin);
	freopen("output.txt", "w", stdout);

	int t;
	cin >> t;
	rep(tt,t)
	{
		rep(i,Z)
			T[0][i] = -1;
		int sz = 1;

		int n;
		cin >> n;
		gets(s);
		int ans = 0;
		rep(i,n)
		{
			gets(s);
			//cout << s << endl;
			int v = 0;

			int j = 0;
			int add = -1;
			while(s[j])
			{
				int c = s[j++] - 'a';
				if(T[v][c] == -1)
				{
					rep(k, Z)
						T[sz][k]  = -1;
					T[v][c] = sz++;
					if(add==-1)add = j;
				}
				v = T[v][c];
			}
			if(add == -1)add = j;

			//cout << add << endl;

			ans += add;
		}
		cout << "Case #" << (tt+1) << ": " << ans << endl;
		//cout << endl;
	}

}