#include <iostream>
#include <stdio.h>
#include <string.h>
using namespace std;
char a[10000006];
int main()
{
    int n;int t;
    scanf("%d",&t);
    for(int i=0;i<t;i++){
        scanf("%d",&n);int y=0;
        for(int j=0;j<n;j++){
            scanf("%s",a);
            y+=strlen(a);
        }
        printf("Case #%d: %d\n",i+1,y);
    }
    return 0;
}
